<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MandalorianoMAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MandalorianoMAUI"
    Shell.FlyoutBehavior="Disabled"
    Title="MandalorianoMAUI"
    xmlns:views="clr-namespace:MandalorianoMAUI.Views">

    <ShellContent
        Title="ListadoMisiones"
        ContentTemplate="{DataTemplate views:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="MisionesCompletadas"
        ContentTemplate="{DataTemplate views:MisionesCompletadas}"
        Route="misionesCompletadas" />
    

</Shell>
